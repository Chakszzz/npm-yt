{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "exclude": ["node_modules"]
  },
  "opts": {
    "destination": "jsdocs",
    "template": "node_modules/clean-jsdoc-theme" 
  },
  "templates": {
    "default": {
      "staticFiles": {
        "include": ["assets"]  
      }
    },
    "clean-jsdoc-theme": {
      "theme": "dark",
      "search": true,
      "linenums": true
    }
  }
}