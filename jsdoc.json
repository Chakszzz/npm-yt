{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "exclude": ["node_modules"]
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "jsdocs",
    "template": "node_modules/better-docs"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "better-docs": {
      "name": "@er-npm/scraper Documentation"
    }
  }
}