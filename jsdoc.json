{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "exclude": ["node_modules"]
  },
  "opts": {
    "destination": "jsdocs",
    "template": "node_modules/ink-docstrap/template"
  },
  "templates": {
    "theme": "cerulean",
    "navType": "inline",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "@er-npm/scraper Docs",
    "footer": "Copyright Â© 2025 - My Project",
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}