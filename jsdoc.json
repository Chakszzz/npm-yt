{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "exclude": ["node_modules"]
  },
  "opts": {
    "destination": "jsdocs"
  },
  "templates": {
    "clean-jsdoc-theme": {
      "theme": "dark",
      "search": true,
      "linenums": true
    }
  },
  "plugins": ["node_modules/clean-jsdoc-theme"]
}